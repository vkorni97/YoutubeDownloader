{
	"name": "youtube-downloader",
	"version": "0.0.0",
	"main": "./main.js",
	"scripts": {
		"ng": "ng",
		"start": "concurrently \"ng serve\" \"wait-on tcp:4200 && electron .\"",
		"build": "npm run webpack:file && ng build --prod --base-href ./ && npm run webpack && npm run electron:builder",
		"build:test": "npm run webpack:file && ng build --base-href ./ && npm run webpack && npm run electron:builder",
		"webpack": "webpack --config webpack.config.js",
		"webpack:file": "webpack --config webpack.file.config.js",
		"electron": "electron .",
		"electron:builder": "electron-builder --c.extraMetadata.main=frontend/main.js -w",
		"git": "git pull && git add . && git commit -m 'Update' && git push origin master",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^11.1.0",
		"@angular/cdk": "^11.0.4",
		"@angular/common": "^11.1.0",
		"@angular/compiler": "^11.1.0",
		"@angular/core": "^11.1.0",
		"@angular/forms": "^11.1.0",
		"@angular/material": "^11.0.4",
		"@angular/platform-browser": "^11.1.0",
		"@angular/platform-browser-dynamic": "^11.1.0",
		"@angular/router": "^11.1.0",
		"electron-clipboard-extended": "^1.1.1",
		"ffmpeg-static": "^4.2.7",
		"ffmpeg-static-electron": "^2.0.1",
		"fluent-ffmpeg": "^2.1.2",
		"ntsuspend": "^1.0.0",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"ytdl-core": "^4.4.5",
		"ytpl": "^2.0.4",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1100.7",
		"@angular/cli": "^11.0.7",
		"@angular/compiler-cli": "^11.1.0",
		"@types/electron": "^1.6.10",
		"@types/ffmpeg-static": "^3.0.0",
		"@types/fluent-ffmpeg": "^2.1.16",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/jest": "^26.0.20",
		"@types/mocha": "^8.2.0",
		"@types/node": "^12.19.9",
		"codelyzer": "^6.0.0",
		"concurrently": "^5.3.0",
		"electron": "^11.2.0",
		"electron-builder": "^22.9.1",
		"electron-reloader": "^1.2.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.1.1",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"node-loader": "^1.0.2",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~4.0.2",
